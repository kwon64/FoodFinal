<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="memberDAO">


<select id="idCheck" parameterType="String" resultType="int" ><!-- 중복확인 -->
	select count(m_id) from foodmember where m_id=#{id}
</select>


<insert id="insertMember" parameterType="member">
insert into foodmember(m_id,m_name,m_pass,m_tel,m_birth,m_area,m_most,m_email) 
values(#{m_id},#{m_name},#{m_pass},#{m_tel},#{m_birth},#{m_area},#{m_most},#{m_email})
</insert>
</mapper>